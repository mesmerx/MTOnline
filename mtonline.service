[Unit]
Description=MTOnline Development Server
After=network.target

[Service]
Type=simple
User=mesmer
WorkingDirectory=/home/mesmer/mtonline
Environment="PATH=/home/mesmer/.nvm/versions/node/v20.19.0/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
Environment="NODE_ENV=development"
ExecStart=/usr/bin/pnpm dev:all
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
















